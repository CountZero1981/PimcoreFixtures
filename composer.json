{
  "name": "youwe/pimcore-fixtures",
  "description": "Load yml fixtures in pimcore",
  "type": "pimcore-bundle",
  "require": {
    "pimcore/pimcore": "^10.0",
    "nelmio/alice": "3.8.*",
    "fakerphp/faker": "^1.10",
    "ext-json": "*",
    "theofidry/alice-data-fixtures": "^1.4"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Manea Eugen"
    },    
    {
      "name": "Joris Ros",
      "email": "j.ros@youwe.nl"
    },
    {
      "name": "Youwe"
    }
  ],
  "autoload": {
    "psr-4": {
      "FixtureBundle\\": "/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "FixtureBundle\\FixtureBundle"
      ]
    }
  }
}
